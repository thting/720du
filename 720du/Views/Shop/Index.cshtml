        #head-slide {
            border-bottom: 0;
        }

        #head-slide {
            position: relative;
            box-sizing: border-box;
            width: 100%;
            background: #ffffff;
            border-bottom: 1px solid #ddd;
        }

        .native-scroll {
            overflow: auto !important;
            -webkit-overflow-scrolling: touch !important;
        }

        .head-slides {
            position: relative;
            width: 100%;
            /* background: #64c1ed; */
            height: 100%;
            z-index: 1;
            display: -webkit-box;
            display: -moz-box;
            display: -ms-flexbox;
            display: -webkit-flex;
            display: flex;
            -webkit-transition-property: -webkit-transform;
            -moz-transition-property: -moz-transform;
            -o-transition-property: -o-transform;
            -ms-transition-property: -ms-transform;
            transition-property: transform;
            -webkit-box-sizing: content-box;
            -moz-box-sizing: content-box;
            box-sizing: content-box;
            color: white;
        }

        .head-slides .slide-items.selected {
            font-weight: 700;
        }

        .head-slides .slide-items {
            font-size: 14px;
            line-height: 40px;
        }


        .head-slides .slide-items {
            user-select: none;
            display: block;
            width: auto;
            float: left;
            position: relative;
            padding: 0 20px;
            color: #333;
            line-height: 44px;
            font-size: 16px;
            text-align: center;
            box-sizing: border-box;
            transition: all ease .3s;
            -webkit-flex-shrink: 0;
            -ms-flex: 0 0 auto;
            flex-shrink: 0;
            z-index: 2;
            padding: 0 10px;
            background: #64c1ed;
            margin-right: 10px;
            font-weight: 700;
            box-shadow: 1px 2px 3px #322e2e;
        }

        .withdrawals-panel {
            border: solid 1px #ccc;
            border-radius: 5px;
            background-color: #fff;
            padding: 0.8rem 0.6rem;
            margin-bottom: 0.3rem;
            overflow: hidden;
